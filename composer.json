{
    "name": "fcnybok/yii-doctrine",
    "description": "Yii3 integration Doctrine extension",
    "type": "library",
    "keywords": ["yii", "doctrine", "orm", "migrations", "fixture"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/fcnybok/yii-doctrine"
    },
    "authors": [
        {
            "name": "Nick Shilo",
            "email": "fcnybok@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "doctrine/orm": "^2.14.1",
        "doctrine/migrations": "^3.6.0",
        "psr/cache": "^2.0|^3.0",
        "symfony/cache": "^6.2.7",
        "psr/log": "^2|^3",
        "yiisoft/aliases": "^3.0",
        "yiisoft/injector": "^1.1",
        "yiisoft/yii-console": "^2.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.6.3",
        "roave/infection-static-analysis-plugin": "^1.25",
        "vimeo/psalm": "^4.30|^5.7",
        "rector/rector": "^0.15.23"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Doctrine\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "di": "di.php",
            "di-console": "di-console.php"
        }
    }
}
